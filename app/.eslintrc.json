{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"plugins": [
		"import",
		"react"
	],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"react-app"
	],
	"settings": {
		"react": {
			"pragma": "React",
			"version": "16"
		}
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"impliedStrict": true,
			"jsx": true
		}
	},
	"rules": {}
}